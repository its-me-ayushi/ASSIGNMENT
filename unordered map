#include<bits/stdc++.h>

using namespace std;

int main()
{
    int n,number;
    cin>>n;
    string name;
    unordered_map<string,int>marks;
    for(int i=0;i<n;i++){
        cin>>name>>number;
        marks[name] = number;
    
    }
cout<<"marks of ayushi"<<marks["ayushi"]<<endl;
cout<<"marks of abhi"<<marks["abhi"]<<endl;
    
}
